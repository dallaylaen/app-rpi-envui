<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <style>
        #t1, #t2, #t3, #t4 { width: 100%; }
    </style>
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script>
        $(function() {
            $('.table').each(function(i, table){
                console.log(
                    $(table).attr('id') + " " +
                    $(table).position().top + " " +
                    $(table).position().left
                );

            });

            $(".td").draggable({
                grid: [10, 1],
                scroll: false,
                stop: function(){
                    var top = $(this).position().top;
                    var left = $(this).position().left;
                    console.log($(this) + " t: " + top + " l: " + left);
                }
            })
        });
    </script>
</head>
<body>

    <table>
        <tr>
            <td><div class="td">Temperature</div></td>
            <td><div class="td">Humidity</div></td>
        </tr>
    </table>
    <table>
        <tr>
            <td><div class="td">test #3</div></td>
            <td><div class="td">test #4</div></td>
        </tr>
    </table>
    <table>
        <tr>
            <td><div class="td">test #5</div></td>
            <td><div class="td">test #6</div></td>
        </tr>
    </table>
    <table>
        <tr>
            <td><div class="td">test #7</div></td>
            <td><div class="td">test #8</div></td>
        </tr>
    </table>
</body>
</html>
